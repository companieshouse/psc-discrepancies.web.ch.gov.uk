{
  "name": "node-web-starter-creed",
  "version": "1.0.0",
  "description": "Node Web Starter for Team Creed",
  "main": "server.js",
  "nyc": {
    "statements": 50,
    "branches": 50,
    "functions": 50,
    "lines": 50,
    "all": true,
    "include": [
      "server/**/*.js"
    ],
    "exclude": [
      "test/**"
    ],
    "check-coverage": true
  },
  "scripts": {
    "build": "gulp build",
    "start": "source ./server/config/.env && gulp build && node server.js",
    "watch": "source ./server/config/.env && nodemon -e js,json,html,njk --watch ./ --config ./config/nodemon.json server.js",
    "test": "source ./server/config/.env && mocha --recursive test/server",
    "coverage": "nyc --check-coverage --reporter=text-summary npm run test",
    "coverage:report": "nyc --reporter=lcov --reporter=text npm run test"
  },
  "author": "Moses Wejuli<mwejuli@companieshouse.gov.uk>",
  "license": "MIT",
  "dependencies": {
    "babel-eslint": "^8.2.6",
    "body-parser": "1.18.3",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.4",
    "govuk-elements-sass": "^3.1.3",
    "govuk-frontend": "^3.2.0",
    "govuk_frontend_toolkit": "^7.6.0",
    "moment": "2.24.0",
    "moment-timezone": "^0.5.23",
    "morgan": "^1.9.0",
    "npm": "^6.9.0",
    "nunjucks": "3.1.7",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.0.0",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.2.1",
    "eslint": "6.8.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "gulp": "4.0.0",
    "gulp-clean": "0.3.2",
    "gulp-concat": "^2.6.1",
    "gulp-exec": "^2.1.3",
    "gulp-minify": "^3.1.0",
    "gulp-sass": "3.2.1",
    "gulp-shell": "^0.6.5",
    "gulp-uglify": "^3.0.2",
    "mocha": "^4.1.0",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "pre-commit": "^1.2.2",
    "sinon": "6.3.5",
    "sinon-chai": "^3.3.0"
  },
  "eslintConfig": ".eslintrc"
}
